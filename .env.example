# Database
DATABASE_URL=postgresql://postgres:password@localhost:5432/digital_university

# Environment
DENO_ENV=development

# Ollama AI (локальный LLM)
OLLAMA_URL=http://localhost:11434
OLLAMA_MODEL=deepseek-v3.1:671b-cloud

# Server
PORT=8000

# Admin (основной ключ)
ADMIN_KEY=dev-admin-key
ADMIN_NAME=Main Admin
ADMIN_EMAIL=admin@example.com

# Дополнительные админы (опционально)
# ADMIN_KEY_1=moderator-key-12345
